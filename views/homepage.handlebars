{{#each post as |post| }}
<section class="post">
    <div class="post-header">
        <div class="post-headline">
            <h3>{{post.title}}</h2>
        </div>
        <div class="post-user-date">
            <img src="../../public/Images/Kevin.jpg" alt="User Photo" class="UserPhoto">
			<h4>Posted by Kevin Bacon</h4>
        </div>
    </div>
    <p>{{post.message}}</p>
</section>
{{/each}}

