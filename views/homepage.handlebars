

<section class="post">
	{{#each posts}}
    <div class="post-header">
        <div class="post-headline">
            <h3>Title: {{title}}</h3>
        </div>
        <div class="post-user">
            <img src="/Images/Kevin.jpg" alt="User Photo" class="UserPhoto">
			<h4>Posted by Kevin Bacon</h4>
        </div>
    </div>
    <p>Message: {{message}}</p>
	{{/each}}
</section>

